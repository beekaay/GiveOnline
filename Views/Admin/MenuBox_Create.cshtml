
@{
    ViewBag.Title = "Event_Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form action="/Admin/MenuBox_Create" method="post" enctype="multipart/form-data" id="menuboxCreate">

    <div class="form-group">
        <label for="title">Caption</label>
        <input type="text" class="form-control" name="title" id="title" required value="" />
    </div>

    <div class="form-group">
        <label for="headline">Headline</label>
        <input class="form-control" name="headline" id="headline" required>
    </div>

    <div class="form-group">
        <label for="imageURL">Image</label>
        <input type="file" class="form-control" name="imageURL" id="imageURL" required value="" />
    </div>

    <div class="form-group">
        <label for="color">Color</label>
        <input type="color" class="form-control" name="color" id="color" required value="" />
    </div>
    

    <button type="submit">Go</button>
</form>



